{
  "name": "confluence-to-bookstack-wizard",
  "version": "1.0.0",
  "description": "Interactive terminal wizard for migrating Confluence spaces to BookStack",
  "author": "Affordable Magic (https://affordablemagic.net)",
  "homepage": "https://github.com/transcriptionstream/confluence-to-bookstack-wizard",
  "repository": {
    "type": "git",
    "url": "https://github.com/transcriptionstream/confluence-to-bookstack-wizard.git"
  },
  "keywords": ["confluence", "bookstack", "migration", "wiki", "import"],
  "license": "MIT",
  "scripts": {
    "setup": "npx tsc && node ./dist/setup.js",
    "web": "npx tsc && node ./dist/server.js",
    "import": "npx tsc && node ./dist/import.js -- import",
    "xml-import": "npx tsc && node ./dist/xml-import.js -- xml-import",
    "sort": "npx tsc && node ./dist/import.js -- sort",
    "attachments": "npx tsc && node ./dist/import.js -- attachments",
    "fixLinks": "npx tsc && node ./dist/import.js -- fixLinks",
    "attach": "npx tsc && node ./dist/attachments.js -- attach",
    "killShelf": "npx tsc && node ./dist/deleteShelf.js -- deleteById",
    "nuke": "node nuke.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "dotenv": "^16.4.3",
    "form-data": "^4.0.0",
    "glob": "^10.5.0",
    "jsdom": "^24.0.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^20.11.18",
    "typescript": "^5.3.3"
  }
}
